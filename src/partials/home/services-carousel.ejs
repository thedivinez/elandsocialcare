<section class="services-section-two">
  <div class="auto-container">
    <!-- Sec Title -->
    <div class="sec-title">
      <h2>Our Training Services</h2>
      <div class="text">
        A dependable, compassionate presence and training you can trust.
      </div>
    </div>

    <div class="services-carousel owl-carousel owl-theme">
      <% site.courses.forEach(function(course) { %>
      <div class="service-block-two">
        <div class="inner-box">
          <div class="image">
            <img src="<%=course.thumbnail%>" alt="" />
          </div>
          <div class="lower-content">
            <h3><a href="<%=course.url%>"><%=course.title%></a></h3>
            <div class="text"><%=course.summary%></div>
          </div>
        </div>
      </div>
      <% }) %>
    </div>
  </div>
</section>
